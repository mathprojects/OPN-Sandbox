            <Operation>1</Operation>
            <LocalCommand/>
            <RemoteCommand/>
            <Overwrite>True</Overwrite>
        </DeployFile>
        <DeployFile Include="OSX64\Release\opnsprop.entitlements" Condition="'$(Config)'=='Release'">
            <RemoteDir>opnsprop.app\..\</RemoteDir>
            <RemoteName>opnsprop.entitlements</RemoteName>
